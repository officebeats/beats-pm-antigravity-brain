{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Open Index.html (Edge)",
      "type": "msedge",
      "request": "launch",
      "file": "${workspaceFolder}/2. Products/[Product]/website_candidate/index.html"
    },
    {
      "name": "Open Index.html (Chrome)",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/2. Products/[Product]/website_candidate/index.html"
    }
  ]
}